{
  "name": "br-takecare-api-server",
  "version": "0.25.0",
  "description": "API Middleware Server",
  "scripts": {
    "====":"===PROD",
    "start": "yarn run server",
    "server": "nodemon --delay 80ms --exec 'node --inspect=0.0.0.0:56745' -r ts-node/register src/index.ts server",
    "===":"===DEV",
    "start:dev": "yarn dockerdown && yarn dockerup",
    "dockerup": "docker-compose --file docker-compose.dev.yml up --build --force-recreate",
    "dockerdown": "docker-compose --file docker-compose.dev.yml down && docker-compose --file docker-compose.dev.yml stop",
    "server:debug": "nodemon --no-deprecation -r ts-node/register src/index.ts server",
    "=====":"===WORKERS/JOBS",
    "exec": "node -r ts-node/register src/index.ts",
    "job:database-migration": "yarn run exec -- job:database-migration",
    "worker:database-migration": "yarn run exec -- worker:database-migration",
    "worker:database-migration:force": "yarn run exec -- worker:database-migration force"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "2.23.0",
    "ws": "7.4.4",
    "axios": "0.19.0",
    "axios-retry": "1.2.0",
    "batch-stream": "0.1.3",
    "class-transformer": "0.2.0",
    "class-validator": "0.13.1",
    "compression": "1.7.3",
    "copyfiles": "2.1.0",
    "core-js": "3.0.0-beta.11",
    "cors": "2.8.5",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "geolib": "2.0.24",
    "gerador-validador-cpf": "3.1.3",
    "graphql": "14.1.1",
    "i18n": "0.8.3",
    "iplocation": "6.1.0",
    "jsonschema": "1.2.4",
    "jsonwebtoken": "8.4.0",
    "lodash": "4.17.11",
    "lodash-move": "1.1.1",
    "memjs": "1.2.2",
    "moment": "2.22.2",
    "moment-timezone": "0.5.27",
    "mongoose": "5.3.13",
    "reflect-metadata": "0.1.12",
    "routing-controllers": "0.7.7",
    "sanitize-html": "1.20.1",
