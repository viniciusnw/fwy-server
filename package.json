  "scripts": {
    "====": "===PROD",
    "start": "yarn run server",
    "server": "node -r ts-node/register src/index.ts server",
    "===": "===DEV",
    "start:dev": "yarn run server:dev",
    "server:dev": "nodemon --legacy-watch --exec 'node --inspect=0.0.0.0:56745' -r ts-node/register src/index.ts server",
    "server:up": "yarn dockerdown && yarn dockerup",
    "dockerup": "docker-compose --file docker-compose.dev.yml up --build --force-recreate",
    "dockerdown": "docker-compose --file docker-compose.dev.yml down && docker-compose --file docker-compose.dev.yml stop",
    "=====": "===WORKERS/JOBS",
    "exec": "node -r ts-node/register src/index.ts",
    "worker:create-admin": "yarn run exec -- worker:create-admin"
  }
}